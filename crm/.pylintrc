[MASTER]
load-plugins=pylint_django
ignore=migrations

[DJANGO]
django-settings-module=crm.settings

[REPORTS]
ignore-patterns=manage.py,apps.py,tests.py

[MESSAGES CONTROL]
disable=raw-checker-failed,
        bad-inline-option,
        locally-disabled,
        file-ignored,
        suppressed-message,
        useless-suppression,
        deprecated-pragma,
        use-symbolic-message-instead,
        use-implicit-booleaness-not-comparison-to-string,
        use-implicit-booleaness-not-comparison-to-zero,
        missing-module-docstring,
        too-many-ancestors